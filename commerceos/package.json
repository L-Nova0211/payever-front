  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "cli": "pe-i18n-cli",
    "start": "nx serve",
    "start:client": "nx serve --project=client",
    "build": "npm run postinstall && nx run main:build:production && npm run i18n:prepare && npm run build:message-embed && npm run build:widgets",
    "test": "nx test",
    "test:all": "nx run-many --all --target test --watch false",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build -- --base=origin/master --parallel",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run client:serve-ssr",
    "serve:ssr": "node dist/builder-client/server/main.js",
    "build:ssr": "nx run client:build:production && nx run client:server:production && nx run consumer:build:production",
    "prerender": "ng run client:prerender",
    "env:prepare": "envsubst < deploy/env.json > src/env.json",
    "precommit": "echo 0",
    "i18n:dev": "npm run i18n:serve",
    "i18n-cli": "node ./bin/pe-i18n-cli.js",
    "i18n:dev:temp": "cd ./dist && mkdir -p i18n && npm run i18n-cli flatten assets/locale/app.json dist/i18n/frontend-commerceos-app-en.json && npm run i18n-cli flatten node_modules/@pe/ng-kit/assets/locale/ng-kit.json dist/i18n/frontend-ng-kit-ng-kit-en.json",
    "i18n:serve": "npm run cli ngcli_i18n && cross-env I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=commerceos && cross-env I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli pull --output-path=./i18n-serve --domain-prefix=commerceos",
    "i18n:prod": "rimraf dist/i18n && cross-env I18N_HOST=translation-backend.staging.devpayever.com pe-i18n-cli push --domain-prefix=commerceos && cross-env I18N_HOST=translation-backend.staging.devpayever.com pe-i18n-cli pull --output-path=./dist/i18n --domain-prefix=commerceos",
    "i18n:prepare": "rimraf dist/translations && node ./bin/pe-i18n-cli.js prepare --domain-prefix=commerceos --prepare-path=dist/translations",
    "build:all": "nx run client:server && nx run-many --all --target=build",
    "build:widgets": "cd ./node_modules/@pe/finexp-app && cp -a widgets/. ../../../dist",
    "build:message-embed": "npm run build:embed && cd ./dist/libs/message/ && cp pe-message-widget.min.js ../../../dist",
    "build:embed": "nx run message-embed:build:production && bin/pe-message-embed-postbuild.js",
    "build:embed-serve": "nx run message-embed:serve",
    "workspace-generator": "nx workspace-generator",
    "build:stats": " npm run postinstall && nx run main:build:bundle --deploy-url=/ --stats-json && npm run i18n:prepare && npm run build:message-embed",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "bundle:analyze": "npm run build:stats && npm run analyze",
    "publish:builder-core": "nx run builder-core:build:publish && cp .npmrc ./dist/libs/builder/core && cd ./dist/libs/builder/core && npm publish",
    "publish:common": "nx run common:build:publish && cp .npmrc ./dist/libs/common && cd ./dist/libs/common && npm publish",
    "publish:media": "nx run media:build:publish && cp .npmrc ./dist/libs/media && cd ./dist/libs/media && npm publish",
    "publish:platform-header": "nx run platform-header:build:publish --with-deps && gulp copy:styles && cp .npmrc ./dist/libs/platform-header && cd ./dist/libs/platform-header && npm publish",
    "publish:snackbar": "nx run snackbar:build:publish --with-deps && gulp copy:styles && cp .npmrc ./dist/libs/snackbar && cd ./dist/libs/snackbar && npm publish",
    "publish:ui": "nx run ui:build:publish --with-deps && gulp copy:styles && cp .npmrc ./dist/libs/ui && cd ./dist/libs/ui && npm publish"
  },
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": "crypto-browserify",
    "stream": "stream-browserify"
